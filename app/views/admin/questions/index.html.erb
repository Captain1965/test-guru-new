<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
<table>
  <p>
    <%= link_to 'Question new', new_admin_test_question_path(@test) %>  
  </p>
  <% @questions.each do |question| %>
    <tr>

      <td><%= question.body %></td><br>
      <td><%= link_to 'Show', admin_question_path(question) %></td>
      <td><%= link_to 'Edit', edit_admin_question_path(question) %></td>
      <td><%= link_to 'Delete',
                  admin_question_path(question),
                  method: :delete,
                  data: {confirm: 'Yuo sure?'} %></td>
    </tr>     
  <% end %> 
</table>