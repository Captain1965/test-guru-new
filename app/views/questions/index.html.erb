
<table>
	<p>
		<%= link_to 'Question new', new_test_question_path(@test) %>	
	</p>
	<% @questions.each do |question| %>
		<tr>
			<td><%= question.body %></td><br>
			<td><%= link_to 'Edit', edit_question_path(question) %></td>
			<td><%= link_to 'Delete',
                  question_path(question),
                  method: :delete,
                  data: {confirm: 'Yuo sure?'} %></td>
		</tr>			
	<% end %>	
</table>
